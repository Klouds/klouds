{{template "header" .}}
{{template "user/usernav" .}}
<div class="page-app">
    <!-- COLOR OVER IMAGE -->
    <!-- /END COLOR OVERLAY -->
    <!-- /END HEADER -->
    <main class="user-main-content">
        <div class="container">
            <div class="row">
                <h1 class="page-header">Application List <small>Look at all the things!</small> </h1>

                {{range $index, $element := .}}
                {{if eq (mod $index 4) 0}}
                    {{end}}
                    <div class="col-md-4 portfolio-item">
                        <a href="#">
                            <img class="img-responsive"  src="{{.Logo}}" alt="{{.Name}}">
                        </a>
                        <h3>
                            <a target="_blank" href="http://{{.AccessUrl}}">{{.Name}}</a>
                        </h3>
                    </div>
                    {{if eq (mod $index 4) 3}}
                {{end}}


                {{end}}
        </div>
        <!-- .content-wrapper -->
    </main>
    <!-- .user-main-content -->
</div>
{{template "footer"}}
